{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
    "theme": "tribe",
    "active": "default",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "default": [
            {
                "name": "server",
                "autoExecuteCommands": true,
                "icon": "cloud",
                "focus": true,
                "color": "terminal.ansiBrightYellow",
                "commands": [
                    ".venv/Scripts/activate; clear; python main.py"
                ]
            },
            {
                "name": "dev",
                "autoExecuteCommands": true,
                "icon": "code",
                "color": "terminal.ansiBrightMagenta",
                "commands": [
                    ".venv/Scripts/activate; clear;"
                ]
            },
            {
                "name": "root",
                "autoExecuteCommands": true,
                "icon": "root-folder",
                "color": "terminal.ansiBrightBlue",
                "commands": [
                    "cd ../; clear"
                ]
            },
            {
                "name": "docker",
                "autoExecuteCommands": true,
                "icon": "symbol-method",
                "color": "terminal.ansiBlue",
                "commands": [
                    "clear; Write-Host 'ALL' -ForegroundColor Yellow; docker ps -a; echo -; Write-Host 'RUN' -ForegroundColor Green; docker ps"
                ]
            }
        ]
    }
}